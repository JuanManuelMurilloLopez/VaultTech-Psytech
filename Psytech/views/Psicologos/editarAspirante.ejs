<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Editar aspirante</title>
    <!-- Bootstrap CSS -->
    <link href=
    "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
    rel="stylesheet">
    <!-- Link css -->
    <link rel="stylesheet" href="/css/psicologos/editarAspirante.css">
    <!-- CSS navside -->
    <link rel="stylesheet" href="/css/includeCss/_navsidePsicologo.css">
    <!-- CSS cerrar sesión -->
    <link rel="stylesheet" href="/css/includeCss/_buttonCerrarSesion.css">

</head>
<body class="bg-light">

    <div class="main">
        <!-- Navside -->
        <%- include('../includes/_navsidePsicologo.ejs') %>

        <div class="container">
        
            <!-- Boton de cerrar sesión -->
            <%- include('../includes/_buttonCerrarSesion.ejs') %>

            <div>
                <h1 class="text-center">EDITAR INFORMACIÓN DE ASPIRANTE</h1>
            </div>
            <div>
                <!--Input nombre-->
                <div class="form-floating mb-3">
                    <input type="" class="form-control" id="" placeholder="">
                    <label for="">Nombre</label>
                </div>
    
                <!--Input Apellido Paterno-->
                <div class="form-floating mb-3">
                    <input type="" class="form-control" id="" placeholder="">
                    <label for="">Apellido Paterno</label>
                </div>
    
                <!--Input Apellido Materno-->
                <div class="form-floating mb-3">
                    <input type="" class="form-control" id="" placeholder="">
                    <label for="">Apellido Materno</label>
                </div>
    
                <!--Input Correo-->
                <div class="form-floating mb-3">
                    <input type="" class="form-control" id="" placeholder="">
                    <label for="">Correo electrónico</label>
                </div>
                <!--Input Número-->
                <div class="form-floating mb-3">
                    <input type="" class="form-control" id="" placeholder="">
                    <label for="">Número de teléfono</label>
                </div>
    
                <!--Input Pais-->
                <select class="form-select" aria-label="Default select example">
                    <option selected>País de origen</option>
                    <option value="1">México</option>
                    <option value="2">Argentina</option>
                    <option value="3">España</option>
                </select>
    
                <br>
    
                <!--Input Estado-->
                <select class="form-select" aria-label="Default select example">
                    <option selected>Estado de origen</option>
                    <option value="1">Querétaro</option>
                    <option value="2">CDMX</option>
                    <option value="3">Monterrey</option>
                    <option value="3">Puebla</option>
                </select>
                <br>
                <!-- Estatus de aspirante -->
                <div>
                    <p></p>
                    <p>Estatus:</p>
                    <div>
                        <p>Activo</p>
                    </div>
                    <button type="button" class="btn btn-primary btn-danger">Desactivar</button>
                </div>
            </div>
    
            <br>
            <div>
                <!-- Boton de navegación "Regresar" -->
                <a href="/psicologo/aspirantes" class="btn btn-color--third">Regresar</a>
    
                <!-- Boton de navegación "Guardar" -->
                <a href="/psicologo/aspirantes" class="btn btn-color--secondary" 
                onclick="confirm('¿Está seguro de guardar los cambios?')">Guardar</a>
            </div>
        </div>
    </div>

    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Incluir el script del navside -->
    <script src="/js/_navside.js"></script>

</body>
</html>