                <div class="dropdown">
                    <a class="btn btn-secondary dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        Escolaridad
                    </a>
                    <ul class="dropdown-menu">
                        <li><a class="dropdown-item" href="#">Licenciatura</a></li>
                        <li><a class="dropdown-item" href="#">Maestría</a></li>
                        <li><a class="dropdown-item" href="#">Doctorado</a></li>
                        <li><a class="dropdown-item" href="#">Otro</a></li>
                    </ul>
                </div>
            </div>

            <!--Input Puesto solicitado-->
            <div class="mb-3">
                <label class="form-label" for="">Puesto solicitado</label>
                <input class="form-control" type="text" name="" id="">
            </div>
        </div>

        <!-- Seccion de datos preguntas -->
        <div>
            <!-- Titulo de seccion -->
            <div class="section-title">
                <h5>Comprensión verbal</h5>
