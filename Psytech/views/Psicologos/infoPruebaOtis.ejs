                <label class="form-label" for="">Edad</label>
                <input class="form-control" type="text" name="" id="">
            </div>

            <!--Input Genero-->
            <div>
                <div class="dropdown">
                    <a class="btn btn-secondary dropdown-toggle" href="#" 
                    role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        Genero
                    </a>
