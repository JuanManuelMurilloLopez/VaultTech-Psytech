<%- include('../includes/_buttonCerrarSesion.ejs') %>
<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Importar Aspirantes</title>

    <!-- Bootstrap CSS -->
    <link href=
    "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" 
    rel="stylesheet">
    <!-- Link css -->
    <link rel="stylesheet" href="/css/psicologos/importarAspirante.css">
</head>

<body class="bg-light">
    <div class="container">
        <h1 class="text-center mt-5">IMPORTAR NUEVO ASPIRANTE</h1>
        
        <p>Instrucciones</p>

        <!-- Archivo -->
            <div class="card">
                <div class="card-header">
                    <h3>Cargar archivo</h3>
                    <p>Solo archivos .cvs con menos de 500 kb</p>
                </div>
                <div class="card-body text-center">
                    <h4>Arrastra para subir</h4>
                    <p>Suelta en este área tu contenido para que se pueda subir</p>
                    
                </div>
                <div class="card-body">
                <br>
                    <h4>Aspirante.cvs</h4>
                    <p>Subiendo (65%)</p>
                    <button type="button" class="btn btn-danger">Eliminar</button>
                </div>
            </div>
        
        <p></p>
        <button type="button" class="btn btn-color--secondary">Subir Archivos</button>
        <p></p>
        <button type="button" class="btn btn-color--secondary"
        onclick="confirm('¿Está seguro de guardar los cambios?'),
        window.location.href=
        '/psicologo/aspirantes'"
        >Guardar Aspirante</button>

        <p></p>
        <!-- Botones de navegación Grupo, Edicion, Regresar" -->
        <button type="button" class="btn btn-color--third"
        onclick="window.location.href=
        '/psicologo/aspirantes'">Regresar</button>
        <br>
    </div>

    <!-- Bootstrap JS -->
    <script src=
    "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    ></script>
</body>